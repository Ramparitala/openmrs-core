node{
     	stage('SCM') {
           //git clone
         git 'https://github.com/openmrs/openmrs-core.git'
       }
      stage(‘Build package’) {
         //Build maven package
        sh label: '', script: 'mvn package'
      }
     stage(‘Archive’) {
       // Archiving artifacts.
       archiveArtifacts 'target/*.jar'  
     }
}
